<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/content_main_layout"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.zissu.noche_1.MainActivity"

    tools:showIn="@layout/app_bar_main"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:layout_centerInParent="true"
        android:id="@+id/imageButton"
        android:layout_width="72dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/res"
        android:onClick="onClickRes"
        />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="72dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/dj"
        android:layout_above="@id/imageButton"
        android:layout_centerHorizontal="true"
        android:onClick="onClickClub"
        />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="72dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/bar"
        android:layout_below="@id/imageButton"
        android:layout_centerHorizontal="true"
        android:onClick="onClickBar"
        />
</RelativeLayout>
